[
  {{#repeat 100}}
  {
    "name" : "{{firstName}} {{lastName}}",
    "age": {{number 18 70}},
    "location" : "{{city}}",
    "gender" : "{{gender}}",
    "picture" : "{{index}}.jpg",
    "cost" : {{number 0 500}},
    "popularity" : {{number 0 99}},
    "keywords" : [{{#repeat 1 3}} "{{keywords}}" {{/repeat}}]
  }
  {{/repeat}}
]