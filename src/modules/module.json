{
  "name": "myapp/modules",
  "myapp/modules/header": {
    "js": [
      "header/app.header.js"
    ],
    "on": [
      "spamjs/view"
    ]
  },
  "myapp/modules/search": {
    "js": [
      "search/app.search.js"
    ],
    "on": [
      "spamjs/view",
      "myapp/modules//profiles",
      "myapp/modules//filters"
    ]
  },
  "myapp/modules/filters": {
    "js": [
      "filters/app.filters.js"
    ],
    "on": [
      "spamjs/view",
      "webmodules/jquery/ui/min",
      "webmodules/jqrouter",
      "jqtags/jq-slider",
      "jqtags/jq-select"
    ]
  },
  "myapp/modules/profiles": {
    "js": [
      "profiles/app.profiles.js"
    ],
    "on": [
      "webmodules/lazy",
      "webmodules/jqrouter",
      "spamjs/view",
      "jqtags/jq-tab",
      "jqtags/jq-select",
      "myapp/modules/service"
    ]
  },
  "myapp/modules/service": {
    "js": [
      "service/app.service.js"
    ],
    "on": [
      "jsutils/file",
      "webmodules/dummy-json"
    ]
  }
}